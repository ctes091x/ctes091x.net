---
import Base from "./Base.astro";
import BlogHeader from "../components/BlogHeader.astro";
const { frontmatter } = Astro.props;
const pubDate = new Date(frontmatter.pubDate);
const modDate = new Date(frontmatter.pubDate);
---

<Base title={frontmatter.title} type="blog" ogImage={frontmatter.ogImage} >
  <article>
    <BlogHeader title={frontmatter.title} pubDate={pubDate} modDate={modDate} />
    <slot />
  </article>
</Base>

